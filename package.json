{
     "name": "flutter-project-setup",
     "displayName": "Flutter Project Setup",
     "description": "Advanced Flutter development toolkit with automated project setup, Clean Architecture templates, feature generation, and comprehensive development workflows. Includes interactive UI panels, Tree View integration, and cross-platform support.",
     "version": "1.1.1",
     "publisher": "DevUnity",
     "engines": {
       "vscode": "^1.90.0"
     },
     "categories": [
     "Snippets",
       "Other",
    "Extension Packs"
  ],
     "keywords": [
     "flutter",
     "dart",
     "project setup",
     "automation",
     "template",
     "feature generator",
     "clean architecture",
       "webview",
    "tree view",
    "ui templates",
    "bloc",
    "state management",
    "localization",
    "unit testing",
    "development workflow",
    "code generator",
    "productivity",
    "cross-platform",
    "mobile development",
    "package management"
  ],
     "repository": {
       "type": "git",
       "url": "https://github.com/Eldeengawy/flutter-project-setup.git"
     },
     "homepage": "https://github.com/Eldeengawy/flutter-project-setup",
     "bugs": {
       "url": "https://github.com/Eldeengawy/flutter-project-setup/issues"
     },
     "activationEvents": [
       "onCommand:flutter-project-setup.showMainPanel",
       "onView:flutterSetupTree"
     ],
     "main": "./dist/extension.js",
     "icon": "icon.png",
     "contributes": {
       "commands": [
         {
           "command": "flutter-project-setup.showMainPanel",
           "title": "Show Flutter Setup Panel"
         },
         {
           "command": "flutter-project-setup.setupProject",
           "title": "Setup Flutter Project"
         },
         {
           "command": "flutter-project-setup.createFeature",
           "title": "Create Flutter Feature"
         },
         {
           "command": "flutter-project-setup.generateUITemplate",
           "title": "Generate UI Template"
         },
         {
           "command": "flutter-project-setup.setupUnitTests",
           "title": "Setup Unit Tests"
         },
         {
           "command": "flutter-project-setup.setupLocalization",
           "title": "Setup Localization"
         }
       ],
       "viewsContainers": {
         "activitybar": [
           {
             "id": "flutter-setup-explorer",
             "title": "Flutter Setup",
             "icon": "media/flutter-icon.svg"
           }
         ]
       },
       "views": {
         "flutter-setup-explorer": [
           {
             "id": "flutterSetupTree",
             "name": "Flutter Tools"
           }
         ]
       },
       "configuration": {
         "title": "Flutter Project Setup",
         "properties": {
           "flutterProjectSetup.defaultPackages": {
             "type": "array",
             "default": ["flutter_bloc", "dartz", "easy_localization"],
             "description": "List of default packages to install during project setup."
           }
         }
       }
     },
     "scripts": {
       "vscode:prepublish": "npm run package",
       "compile": "npm run check-types && npm run lint && node esbuild.js",
       "watch": "npm-run-all -p watch:*",
       "watch:esbuild": "node esbuild.js --watch",
       "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
       "package": "npm run check-types && npm run lint && node esbuild.js --production",
       "compile-tests": "tsc -p . --outDir out",
       "watch-tests": "tsc -p . -w --outDir out",
       "pretest": "npm run compile-tests && npm run compile && npm run lint",
       "check-types": "tsc --noEmit",
       "lint": "eslint src",
       "test": "vscode-test"
     },
     "devDependencies": {
       "@types/vscode": "^1.74.0",
       "@types/mocha": "^10.0.10",
       "@types/node": "22.x",
       "@typescript-eslint/eslint-plugin": "^8.42.0",
       "@typescript-eslint/parser": "^8.42.0",
       "eslint": "^9.34.0",
       "esbuild": "^0.25.9",
       "npm-run-all": "^4.1.5",
       "typescript": "^5.9.2",
       "@vscode/test-cli": "^0.0.11",
       "@vscode/test-electron": "^2.5.2"
     }
   }
