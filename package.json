{
  "name": "flutter-project-setup",
  "displayName": "Flutter Project Setup",
  "description": "Automates Flutter project setup with predefined tasks and essential packages",
  "version": "1.0.3",
  "publisher": "DevUnity",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "flutter",
    "dart",
    "project setup",
    "automation",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Eldeengawy/flutter-project-setup.git"
  },
  "homepage": "https://github.com/Eldeengawy/flutter-project-setup",
  "bugs": {
    "url": "https://github.com/Eldeengawy/flutter-project-setup/issues"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "icon.png",
  "contributes": {
    "commands": [
      {
        "command": "flutter-project-setup.setupProject",
        "title": "Setup Flutter Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
